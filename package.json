    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^4.1.2",
    "chunk-manifest-webpack-plugin": "1.1.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.9",
    "css-modules-require-hook": "^4.0.1",
    "cssnano": "^3.7.0",
    "enzyme": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-ava": "^4.5.0",
    "eslint-plugin-import": "^2.8.0",
